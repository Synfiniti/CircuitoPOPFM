{
  "expo": {
    "name": "circuito-pop",
    "slug": "circuito-pop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "circuitopop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.synfiniti.circuitopop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-ios.png",
        "backgroundImage": "./assets/images/icon-background.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.synfiniti.circuitopop"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-fake.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#be00b9"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1fda9f1-a9f8-4c9e-98b7-cb4db74c5a20"
      }
    }
  }
}
